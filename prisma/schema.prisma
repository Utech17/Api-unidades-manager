generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Model {
  modelCode   String   @id
  description String

  units       Unit[]
}

model Unit {
  plate       String   @id
  description String
  modelCode   String
  seatCount   Int
  type        String
  year        Int
  
  model       Model    @relation(fields: [modelCode], references: [modelCode])
}
